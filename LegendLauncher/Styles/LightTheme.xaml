<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



    <SolidColorBrush x:Key="BackgroundPrimaryColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="BackgroundSecondaryColor" Color="#FFDADADA"/>

    <SolidColorBrush x:Key="BackgroundBorderColor" Color="#E0E0E0"/>

    <Color x:Key="BorderShadowEffectColor">Gray</Color>

    <SolidColorBrush x:Key="ForegroundPrimaryColor" Color="Black"/>
    <SolidColorBrush x:Key="ForegroundSecondaryColor" Color="Black"/>


</ResourceDictionary>